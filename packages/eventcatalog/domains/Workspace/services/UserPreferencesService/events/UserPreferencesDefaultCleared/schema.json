{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "UserPreferencesDefaultCleared",
	"type": "object",
	"title": "UserPreferencesDefaultCleared",
	"description": "Recorded when a Data Analyst clears their default catalog preference.",
	"required": ["preferencesId", "clearedAt"],
	"properties": {
		"preferencesId": {
			"type": "string",
			"format": "uuid",
			"description": "Preferences being updated"
		},
		"previousCatalogUri": {
			"type": "string",
			"format": "uri",
			"description": "Catalog URI that was cleared"
		},
		"clearedAt": {
			"type": "string",
			"format": "date-time",
			"description": "Timestamp when the default was cleared"
		}
	}
}
