{
	"$schema": "https://json-schema.org/draft/2020-12/schema",
	"$id": "RenameSavedQuery",
	"type": "object",
	"title": "RenameSavedQuery",
	"description": "Command to change the display name of an existing saved query.",
	"required": ["savedQueryId", "newName"],
	"properties": {
		"savedQueryId": {
			"type": "string",
			"format": "uuid",
			"description": "Saved query to rename"
		},
		"newName": {
			"type": "string",
			"description": "New display name"
		}
	}
}
